{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 23,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "default",
      "displayName": "Default Config",
      "description": "Configure",
      "binaryDir": "${sourceDir}/../build"
    }
  ],
  "buildPresets": [
    {
      "name": "default",
      "displayName": "default",
      "configurePreset": "default",
      "configuration": "Debug"
